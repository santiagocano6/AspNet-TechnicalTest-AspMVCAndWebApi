@model IEnumerable<GAP.Models.ArticuloModels>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalEnEstante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalEnBodega)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalEnEstante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalEnBodega)
        </td>
    </tr>
}

</table>
